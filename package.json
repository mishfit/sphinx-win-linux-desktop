{
  "name": "sphinx",
  "description": "Bitcoin Lightning Powered Chat",
  "author": "Evan Feenstra",
  "version": "0.1.0",
  "main": "public/electron",
  "homepage": "./",
  "scripts": {
    "start": "react-native start",
    "web": "react-app-rewired start",
    "build": "react-app-rewired build",
    "electron": "ELECTRON_DEV_URL=http://localhost:3000 electron .",
    "install-app-deps": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist:linux": "./node_modules/.bin/electron-builder --linux",
    "dist:win": "./node_modules/.bin/electron-builder --win",
    "test": "mocha"
  },
  "rnpm": {
    "assets": [
      "./android_assets/fonts"
    ]
  },
  "build": {
    "appId": "sphinx.chat",
    "productName": "Sphinx Chat",
    "copyright": "copyright Â© 2020 Stakwork",
    "win": {
      "target": "nsis",
      "publisherName": "Stakwork",
      "icon": "build/static/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "identity": null,
      "category": "com.stakwork.sphinx.chat",
      "icon": "build/static/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "build/static/icon.png",
      "category": "Finance",
      "desktop": {
        "Name": "Sphinx",
        "Comment": "Lightning Chat",
        "Categories": "Network;Finance"
      }
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "build"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@cleandersonlobo/react-mic": "^1.2.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@react-native-community/async-storage": "^1.12.1",
    "@types/react-dom": "^16.9.8",
    "assert": "^1.5.0",
    "base-64": "^0.1.0",
    "browserify-zlib": "^0.1.4",
    "bs58-rn": "0.0.3",
    "buffer": "^5.6.0",
    "chart.js": "^2.9.4",
    "console-browserify": "^1.2.0",
    "constants-browserify": "^1.0.0",
    "dns.js": "^1.0.1",
    "domain-browser": "^1.2.0",
    "dompurify": "^2.0.15",
    "electron-deeplink": "^1.0.6",
    "electron-default-menu": "^1.0.2",
    "electron-log": "^4.2.2",
    "electron-unhandled": "^3.0.2",
    "emoji-mart": "^3.0.0",
    "eventemitter3": "^4.0.4",
    "events": "^1.1.1",
    "form-data": "^3.0.0",
    "formik": "^2.1.4",
    "howler": "^2.2.1",
    "https-browserify": "0.0.1",
    "install": "^0.13.0",
    "ion-sdk-js": "^0.3.4",
    "jscryptor-2": "0.0.1",
    "keytar": "^6.0.1",
    "localforage": "^1.8.1",
    "mobx": "^5.15.4",
    "mobx-persist": "^0.4.1",
    "mobx-react-lite": "^1.5.2",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "node-fetch": "^2.6.0",
    "npm": "^6.14.6",
    "open": "^7.2.1",
    "paper": "^0.12.4",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "protoo-client": "^4.0.3",
    "punycode": "^1.4.1",
    "qrcode.react": "^1.0.0",
    "qrious": "^4.0.2",
    "querystring-es3": "^0.2.1",
    "react": "16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-audio-player": "^0.14.0",
    "react-dom": "^16.9.0",
    "react-draggable": "^4.4.3",
    "react-dropzone": "^11.0.3",
    "react-giphy-searchbox": "^1.4.0",
    "react-h5-audio-player": "^3.4.1",
    "react-jutsu": "^2.3.0",
    "react-linkify": "^1.0.0-alpha",
    "react-lottie": "^1.2.3",
    "react-moment": "^0.9.7",
    "react-navigation": "^4.3.7",
    "react-scripts": "^3.4.1",
    "react-tiny-link-electron": "^3.6.1",
    "readable-stream": "^1.0.33",
    "rn-fetch-blob": "^0.12.0",
    "rn-rncryptor": "^1.0.12",
    "sdp-transform": "^2.14.0",
    "socket.io-client": "^2.3.0",
    "stream-browserify": "^1.0.0",
    "string_decoder": "^0.10.31",
    "styled-components": "^5.1.1",
    "timers-browserify": "^1.4.2",
    "ts-import-plugin": "^1.6.6",
    "tty-browserify": "0.0.0",
    "url": "^0.10.3",
    "util": "^0.10.4",
    "uuid": "^7.0.3",
    "video-react": "^0.14.1",
    "vm-browserify": "0.0.4",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@iconify/icons-mdi": "^1.0.148",
    "@iconify/react": "^1.1.3",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.23",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "customize-cra": "^1.0.0",
    "electron": "^9.2.0",
    "electron-builder": "^22.7.0",
    "mocha": "^8.2.1",
    "react-dev-utils": "^10.2.1",
    "react-test-renderer": "16.13.1",
    "spectron": "^11.1.0",
    "typescript": "~3.6.3"
  },
  "private": true,
  "browser": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
